{
  "expo": {
    "name": "ChowNow",
    "slug": "ChowNow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./assets/images/splash/DarkCN.png",
        "light": "./assets/images/splash/LightCN.png",
        "tinted": "./assets/images/splash/TintedCN.png"
      },
      "bundleIdentifier": "com.letamletam.chownow",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash/splash-icon-lightCN.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F97216",
          "dark": {
            "image": "./assets/images/splash/splash-icon-darkCN.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/DMSans.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ef891668-0bd2-467c-8f74-5b376a69c94c"
      }
    }
  }
}
